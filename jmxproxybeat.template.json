{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": false
      },
      "_meta": {
        "version": "5.2.2"
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "bean": {
          "properties": {
            "attribute": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "java/lang:type=ClassLoading": {
              "properties": {
                "LoadedClassCount": {
                  "type": "long"
                },
                "TotalLoadedClassCount": {
                  "type": "long"
                },
                "UnloadedClassCount": {
                  "type": "long"
                }
              }
            },
            "java/lang:type=Compilation": {
              "properties": {
                "TotalCompilationTime": {
                  "type": "float"
                }
              }
            },
            "java/lang:type=Memory": {
              "properties": {
                "HeapMemoryUsage": {
                  "properties": {
                    "committed": {
                      "type": "float"
                    },
                    "init": {
                      "type": "float"
                    },
                    "max": {
                      "type": "float"
                    },
                    "used": {
                      "type": "float"
                    }
                  }
                },
                "NonHeapMemoryUsage": {
                  "properties": {
                    "committed": {
                      "type": "float"
                    },
                    "init": {
                      "type": "float"
                    },
                    "max": {
                      "type": "float"
                    },
                    "used": {
                      "type": "float"
                    }
                  }
                }
              }
            },
            "java/lang:type=OperatingSystem": {
              "properties": {
                "AvailableProcessors": {
                  "type": "long"
                },
                "CommittedVirtualMemorySize": {
                  "type": "float"
                },
                "FreePhysicalMemorySize": {
                  "type": "float"
                },
                "FreeSwapSpaceSize": {
                  "type": "float"
                },
                "MaxFileDescriptorCount": {
                  "type": "long"
                },
                "ProcessCpuLoad": {
                  "type": "float"
                },
                "ProcessCpuTime": {
                  "type": "long"
                },
                "SystemCpuLoad": {
                  "type": "float"
                },
                "SystemLoadAverage": {
                  "type": "float"
                },
                "TotalPhysicalMemorySize": {
                  "type": "float"
                },
                "TotalSwapSpaceSize": {
                  "type": "float"
                }
              }
            },
            "java/lang:type=Threading": {
              "properties": {
                "CurrentThreadCpuTime": {
                  "type": "float"
                },
                "CurrentThreadUserTime": {
                  "type": "float"
                },
                "DaemonThreadCount": {
                  "type": "long"
                },
                "PeakThreadCount": {
                  "type": "long"
                },
                "ThreadCount": {
                  "type": "long"
                },
                "TotalStartedThreadCount": {
                  "type": "long"
                }
              }
            },
            "java/nio:type=BufferPool,name=direct": {
              "properties": {
                "Count": {
                  "type": "float"
                },
                "MemoryUsed": {
                  "type": "float"
                },
                "TotalCapacity": {
                  "type": "float"
                }
              }
            },
            "java/nio:type=BufferPool,name=mapped": {
              "properties": {
                "Count": {
                  "type": "float"
                },
                "MemoryUsed": {
                  "type": "float"
                },
                "TotalCapacity": {
                  "type": "float"
                }
              }
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "value": {
              "type": "float"
            }
          }
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "meta": {
          "properties": {
            "cloud": {
              "properties": {
                "availability_zone": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "instance_id": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "machine_type": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "project_id": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "provider": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "region": {
                  "ignore_above": 1024,
                  "type": "keyword"
                }
              }
            }
          }
        },
        "tags": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.mapping.total_fields.limit": 10000,
    "index.refresh_interval": "5s"
  },
  "template": "jmxproxybeat-*"
}