{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": false
      },
      "_meta": {
        "version": "5.2.2"
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "bean": {
          "properties": {
            "Catalina": {
              "properties": {
                "GlobalRequestProcessor": {
                  "properties": {
                    "bytesReceived": {
                      "type": "long"
                    },
                    "bytesSent": {
                      "type": "long"
                    },
                    "errorCount": {
                      "type": "long"
                    },
                    "maxTime": {
                      "type": "long"
                    },
                    "processingTime": {
                      "type": "long"
                    },
                    "requestCount": {
                      "type": "long"
                    }
                  }
                },
                "ThreadPool": {
                  "properties": {
                    "acceptorThreadCount": {
                      "type": "long"
                    },
                    "connectionCount": {
                      "type": "long"
                    },
                    "currentThreadCount": {
                      "type": "long"
                    },
                    "currentThreadsBusy": {
                      "type": "long"
                    },
                    "maxConnections": {
                      "type": "long"
                    },
                    "maxThreads": {
                      "type": "long"
                    },
                    "minSpareThreads": {
                      "type": "long"
                    }
                  }
                }
              }
            },
            "domain": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "full_name": {
              "norms": false,
              "type": "text"
            },
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "java": {
              "properties": {
                "lang": {
                  "properties": {
                    "ClassLoading": {
                      "properties": {
                        "LoadedClassCount": {
                          "type": "long"
                        },
                        "TotalLoadedClassCount": {
                          "type": "long"
                        },
                        "UnloadedClassCount": {
                          "type": "long"
                        }
                      }
                    },
                    "Compilation": {
                      "properties": {
                        "TotalCompilationTime": {
                          "type": "float"
                        }
                      }
                    },
                    "GarbageCollector": {
                      "properties": {
                        "CollectionCount": {
                          "type": "long"
                        },
                        "CollectionTime": {
                          "type": "long"
                        },
                        "LastGcInfo": {
                          "properties": {
                            "GcThreadCount": {
                              "type": "long"
                            },
                            "duration": {
                              "type": "long"
                            },
                            "endTime": {
                              "type": "long"
                            },
                            "id": {
                              "type": "long"
                            }
                          }
                        }
                      }
                    },
                    "Memory": {
                      "properties": {
                        "HeapMemoryUsage": {
                          "properties": {
                            "committed": {
                              "type": "float"
                            },
                            "init": {
                              "type": "float"
                            },
                            "max": {
                              "type": "float"
                            },
                            "used": {
                              "type": "float"
                            }
                          }
                        },
                        "NonHeapMemoryUsage": {
                          "properties": {
                            "committed": {
                              "type": "float"
                            },
                            "init": {
                              "type": "float"
                            },
                            "max": {
                              "type": "float"
                            },
                            "used": {
                              "type": "float"
                            }
                          }
                        }
                      }
                    },
                    "MemoryPool": {
                      "properties": {
                        "PeakUsage": {
                          "properties": {
                            "committed": {
                              "type": "float"
                            },
                            "init": {
                              "type": "float"
                            },
                            "max": {
                              "type": "float"
                            },
                            "used": {
                              "type": "float"
                            }
                          }
                        },
                        "Usage": {
                          "properties": {
                            "committed": {
                              "type": "float"
                            },
                            "init": {
                              "type": "float"
                            },
                            "max": {
                              "type": "float"
                            },
                            "used": {
                              "type": "float"
                            }
                          }
                        }
                      }
                    },
                    "OperatingSystem": {
                      "properties": {
                        "AvailableProcessors": {
                          "type": "long"
                        },
                        "CommittedVirtualMemorySize": {
                          "type": "float"
                        },
                        "FreePhysicalMemorySize": {
                          "type": "float"
                        },
                        "FreeSwapSpaceSize": {
                          "type": "float"
                        },
                        "MaxFileDescriptorCount": {
                          "type": "long"
                        },
                        "ProcessCpuLoad": {
                          "type": "float"
                        },
                        "ProcessCpuTime": {
                          "type": "long"
                        },
                        "SystemCpuLoad": {
                          "type": "float"
                        },
                        "SystemLoadAverage": {
                          "type": "float"
                        },
                        "TotalPhysicalMemorySize": {
                          "type": "float"
                        },
                        "TotalSwapSpaceSize": {
                          "type": "float"
                        }
                      }
                    },
                    "Runtime": {
                      "properties": {
                        "StartTime": {
                          "type": "long"
                        },
                        "Uptime": {
                          "type": "long"
                        }
                      }
                    },
                    "Threading": {
                      "properties": {
                        "CurrentThreadCpuTime": {
                          "type": "float"
                        },
                        "CurrentThreadUserTime": {
                          "type": "float"
                        },
                        "DaemonThreadCount": {
                          "type": "long"
                        },
                        "PeakThreadCount": {
                          "type": "long"
                        },
                        "ThreadCount": {
                          "type": "long"
                        },
                        "TotalStartedThreadCount": {
                          "type": "long"
                        }
                      }
                    }
                  }
                },
                "nio": {
                  "properties": {
                    "BufferPool": {
                      "properties": {
                        "Count": {
                          "type": "float"
                        },
                        "MemoryUsed": {
                          "type": "float"
                        },
                        "TotalCapacity": {
                          "type": "float"
                        }
                      }
                    }
                  }
                }
              }
            },
            "name": {
              "norms": false,
              "type": "text"
            },
            "type": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "meta": {
          "properties": {
            "cloud": {
              "properties": {
                "availability_zone": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "instance_id": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "machine_type": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "project_id": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "provider": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "region": {
                  "ignore_above": 1024,
                  "type": "keyword"
                }
              }
            }
          }
        },
        "tags": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.mapping.total_fields.limit": 10000,
    "index.refresh_interval": "5s"
  },
  "template": "jmxproxybeat-*"
}